<%- include('partials/header') %>

<div class="jumbotron centered">
  <div class="container">
    <i class="fa fa-key fa-6x"></i>
    <h1 class="display-3">You have discovered my secret.</h1>

    <% usersWithSecrets.forEach(function(user){ %>
    <p class="secret-text"><%= user.secret %></p>
    <% }) %>

    <hr />
    <a href="/logout" role="button" class="btn btn-light btn-lg">Logout</a>
    <a href="/submit" role="button" class="btn btn-dark btn-lg"
      >Submit a Secret</a
    >
  </div>
</div>

<%- include('partials/footer') %>
